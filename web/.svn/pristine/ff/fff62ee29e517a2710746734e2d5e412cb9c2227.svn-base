*{
    margin:0;padding: 0;
    list-style: none;
    font-family:楷体;
    font-size:18px;
}
body{
    background:url(../img/3.jpg) no-repeat 0 0;
    background-size:cover;
    overflow:hidden;
}
canvas{
    background:#fff;
}
.canvasbox{
    width:100%;
    height:85%;
    position:relative;
}
.copy{
    width:100%;height:100%;
    position:absolute;
    z-index:2;
    top:0;
    left:0;
}
.box{
    width:80%;height:80%;
    border:1px solid #333;
    position:absolute;
    margin:auto auto;
    left:0;right:0;top:0;
    bottom: 0;
}
.container{
    width:100%;
    height:50px;
    background:#584027;
    text-align:center;
    line-height:50px;
    position:relative;
    z-index:9;
}
.top{
    width:12.5%;
    height:100%;
    float:left;
    color:#fff;
    cursor:pointer;
    position:relative;
}
.g{
    background: #ffa714;
}
.xuan{
    width:100%;
    height:auto;
    background: #584027;
    display:none;
    position:absolute;
    left:0;
}
.slide:hover{
    background: #EEA236;
}
.xw{
    width:35px;
    height:15px;
    font-size:10px;
}
.num{
    width:35px;
    height:15px;
    font-size: 10px;
}
.xpinput{
    width:80%;
}
.xp{
    width:15px;
    height:15px;
    border:1px solid #000;
    position:absolute;
    top:0;
    left:0;
    display:none;
}
