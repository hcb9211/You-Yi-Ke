
/*?????*/
.bn{
    width: 100%;
    height: 260px;
    background:#f2ecec;
    cursor:pointer
}
.box{
    width: 1200px;
    height: 180px;
    margin:0 auto;
}
.box .top{
    width:300px;
    height:210px;
    margin-top: 30px;
}
.cptx{
    width: 300px;
    height: 90px;
    text-align: center;
    line-height:90px;
    background: #00a885;
    color: #f7f3f3;
    position: relative;
    top:30px;
    font-size: 20px;
}
.cptx img{
    position: absolute;
    top:25px;
    left: 50px;
    width: 40px;
    height: 40px;
}
.top .btn{
    width:300px;
    height:70px;
    background: #fcfeff;
    text-align: center;
    line-height:70px;
}
.btn a{
    width:300px;
    height:70px;
    background: #fcfeff;
    text-align: center;
    line-height:70px;
    display: block;
}
.box .right{
    position: relative;
}
.right .con{
    width:860px;
    height:700px;
    position:absolute;
    top:-300px;
    left:330px;
}
.top .active{
    border-left: 5px solid #00a885;
    background: #d5d5d5;
    width: 295px;
    color:#00a885 ;
}
.yindao{
    width: 860px;
    height: 80px;
    line-height: 80px;
    background:#FFFFFF;
}
.yindao p{
    margin-left: 20px;
    color: #828282;
}
.yindao p a{
    color: #828282;
}
.content{
width:860px;
height:800px;
background: #ffffff;
}
.ditu{
    width: 850px;
    height: 450px;
    border:1px solid #ccc;
    margin:0 auto;
}
#map{
    width:100%;
    height: 100%;
    overflow: hidden;margin:0;
}

.dizhi{
    width: 820px;
    height:200px ;
    margin: 20px auto;
}
.dizhi p{
    width: 820px;
    height: 50px;
    line-height: 40px;
    color: #828282;
}
.content .form-group{
    margin-bottom:10px;
    width: 860px;
    line-height: 100px;
    text-align: center;
}

.content .form-group label{
    display: inline-block;
    text-align: right;
    /*padding-right:10px;*/
    font-size:16px;
    color: #666;
    position: relative;
}
.content .form-group label img{
    position: relative;
    left: -10px;
    top:5px;
}
.content .form-group label i {
    display: inline-block;
    padding-right: 5px;
    font-style: normal;
    color: #ff3734;
    vertical-align: middle;
}
.content .form-group input{
    width: 400px;
    height: 40px;
}
.content .form-group textarea{
    width: 400px;
    height: 80px;
    margin-top: 40px;
    vertical-align: top;
    max-height:100px;
}
.content .form-group select{
    width: 400px;
    height: 40px;
}
.content .form-group #wt{
   text-align: center;
    color: #666;
}
