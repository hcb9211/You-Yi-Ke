@media screen and (max-width:750px){
    .bn{
        width: 100%;
        height: 750px;
        background:#f2ecec;
        cursor:pointer
    }
    .box{
        width: 100%;
        height: auto;
        margin:0 auto;
    }
    .box .top{
       display: none;
    }
    .cptx{
        display: none;
    }
    .box .right{
        position: relative;
    }
    .right .con{
        width:100%;
        height:700px;
        position:absolute;
        top:0;
        left:0;
    }
    .top .active{
        border-left: 5px solid #00a885;
        background: #d5d5d5;
        width: 295px;
        color:#00a885 ;
    }
    .yindao{
        width: 100%;
        height: 80px;
        line-height: 80px;
        background:#FFFFFF;
    }
    .yindao p{
        margin-left: 20px;
        color: #828282;
    }
    .yindao p a{
        color: #828282;
    }
    .content{
        width:100%;
        height:670px;
        background: #ffffff;
    }
    .ditu{
       display: none;
    }
    .dizhi{
       display: none;
    }
    .content form{
        width: 100%;
        height:650px;
    }
    .content .form-group{
        width: 95%;
        margin: 0 auto;
        line-height: 80px;
        text-align: center;
        display: block;
    }

    .content .form-group label{
        font-size:16px;
        height: 30px;
        color: #666;
        position: relative;
    }
    .content .form-group label img{
        position: relative;
        left: -10px;
        top:3px;
        width: 18px;
        height: 15px;
    }
    .content .form-group label i {
        display: inline-block;
        padding-right: 5px;
        font-style: normal;
        color: #ff3734;
        vertical-align: middle;
    }
    .content .form-group input{
        width: 170px;
        height: 30px;
    }
    .content .form-group textarea{
        width: 170px;
        height: 60px;
        margin-top: 40px;
        vertical-align: top;
    }
    .content .form-group select{
        width: 170px;
        height: 40px;
    }
    .content .form-group #wt{
        text-align: center;
        color: #666;
    }

    .form-group button{
        width:100px;
        height: 40px;
        color: #999999;
        background: #f7f7f7;
        border:1px solid #aaaaaa;
        margin-left: 20px;
    }
    /*????*/
    .lianxi{
     display: none;
    }


    /*活动*/
    .huod{
      display: none;
    }

}






















