}
.left-con-1 li:nth-child(1):before{
  content:"\e604";
}
.left-con-1 li:nth-child(2):before{
  content:"\e603";
}
.left-con-1 li:nth-child(3):before{
  content:"\e600";
}
.left-con-1 li:nth-child(4):before{
  content:"\e601";
}
.left-con-1 li:nth-child(5):before{
  content:"\e602";
}
.left-con-1 li:nth-child(6):before{
  content:"\e605";
}

.swiper-container{
  width:348px;height:456px;
  float:right;
}
.swiper-slide img{
  display:block;
  width:348px;height:456px;
}
.pingjia{
  width:330px;height:180px;
  position:relative;
  top:40px;right:0!important;
}
.pj-title{
  color:#3B5998;
  text-align:center;
  font-size:18px;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.pj-con{
  width:330px;height:155px;
}
.pj-con pre{
  color:#717171;
  font-size:14px;
  font-weight:500;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
/*滚动条*/
.mid-scroll{
  width:15px;height:580px;
