@keyframes jianjie {
  0%{
    opacity:0;width:0;
  }
  25%{
    opacity:0.25;width:180px;
  }
  50%{
    opacity:0.5;width:360px;
  }
  75%{
    opacity:0.75;width:540px;
  }
  100%{
    opacity:1;width:720px;
  }
}
