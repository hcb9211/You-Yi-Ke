@background:url(../img/bg/bg5.png) ;
body,input,form,h1,h2,h3,h4,h5,h6,p,ul,li,dl,dd,dt{
  margin:0;
  padding:0;
}
a{
  text-decoration: none;
  border:0;
}
img{
  border:0;
  vertical-align: middle;
}
ul,li{list-style: none;};
@font-face {font-family: 'iconfont';
  src: url('../icon/iconfont.eot'); /* IE9*/
  src: url('../icon/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('../icon/iconfont.woff') format('woff'), /* chrome、firefox */
  url('../icon/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('../icon/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}

body{
  background:@background;
  font-family:'微软雅黑';
  font-weight: 400;
  font-size: 13px;
  line-height: 25px;
  color: #333;
  overflow-x: hidden;
}
.wrapper{
  display:block;
  width:100%;
  height:auto;
  overflow: hidden;
  position:absolute;top:50%;
  margin:-290px auto 0 auto;
  transition:all .5s ease;
 }
.tab-content{
  display:block;
  width:100%;height:580px;
}

.container{
  padding-left:15px;
  padding-right:15px;
  margin-left:auto;
  margin-right:auto;
}
.row{
  width:100%;height:auto;
  padding-left:-15px;padding-right:-15px;
}

.container div:last-child{
  margin:0;
}
.con-left{
  margin-right:15px;
}
           /*共有属性*/

/*左边str*/
.con-left{
  width:78px;height:100%;float:left;
}
.con-left-top{
  width:76px;height:383px;
  border:1px solid #8D8D8D;
  margin-bottom: 40px;
  cursor:pointer;
}
.con-left-top li{
  width:60px;height:60px;
  border-bottom:1px solid #69C5C1;
  background: #3b5998;
  transition:all 1s ease;
  padding-left:16px;padding-top:16px;
}
.con-left-top li:hover{
  background:white;
}
.con-left-top li:last-child{
  border:0;
}
.con-left-bom li:last-child{
  border:0;
}
.con-left-top li span{
  display:block;
  width:44px;height:44px;
  border-radius:50%;
  background:white;
  transition:all 1s ease;
  font-family: 'iconfont';
  text-align:center;
  line-height:44px;
  color:#3B5998;
  font-size:20px;
  transition:all .5s ease;
}
.con-left-top li:hover span{
  background:#3B5998;
  color:white;
  font-size:30px;
}
.tile-1:after{
  content:"\e604";
}
.tile-2:after{
  content:"\e607";
}
.tile-3:after{
  content:"\e608";
}
.tile-4:after{
  content:"\e606";
}
.tile-5:after{
  content:"\e601";
}
.con-left-bom{
  width:76px;height:153px;
  border:1px solid #8D8D8D;
  cursor:pointer;
}
.con-left-bom li{
  width:76px;height:76px;
  background:#3B4449;
  border-bottom:1px solid #62696D;
  transition:all 1s ease;
  text-align: center;
  line-height:76px;
}
.con-left-bom li:hover{
  background:#2D393F;
}
.con-left-bom li span{
  font-family:"iconfont";
  color:white;
  font-size:25px;
}
.tile-6:after{
  content:"\e60a";
}
.tile-7:after{
  content:"\e609";
  font-size:30px;
}
/*左边end*/
/*中间str*/
.middlebox{
  width:645px;height:580px;position:relative;
  float:left;z-index:111;
  left:0;top:0;
  transition:all .5s ease;
}
.m_con{
  position:absolute;
  left:0;top:0;z-index:111;
}
.m_con,.m_con{
  width:750px;height:100%;float:left;
  background: #F4F4F4;
  position:absolute;
  left:0;top:0;
}
.mid-content,.mid-content-jnzs{
  height:auto;
  padding:0 30px 0 15px;
}
.con-title{
  width:100%;height:53px;
  border-bottom:1px solid #E6E6E6;
}
.con-title span{
  display:block;
  color:#3B5998;
  font-size:20px;
  line-height:53px;
}
.con-1{
