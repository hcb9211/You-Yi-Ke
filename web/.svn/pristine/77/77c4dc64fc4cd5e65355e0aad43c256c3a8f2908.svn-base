}
.searchMiddle input{
	width:481px;height:36px;
	border:2px solid #C40000;
	color:#666;font-size:14px;
	float:left;
	padding-left:10px;
	outline: none;
}
.searchMiddle button{
	width:130px;height:40px;
	background:#c40000;
	text-align: center;
	line-height:40px;
	color:white;
	font-size:16px;font-weight: bold;
