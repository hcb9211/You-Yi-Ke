/*导航栏*/
.navbox{
	width:100%;height:27px;
	border-bottom:1px solid #E5E5E5;
}
.nav{
	width:1230px;height:27px;
	margin:0 auto;
}
.navLeft{
	width:390px;height:27px;
	float: left;
}
.navRight{
	width:565px;height:27px;
	float:right;
}
.navLeft li{
	width:65px;height:27px;
	float: left;
	text-align: center;
	line-height:27px;
	color:#999;font-size:12px;
}
.navLeft li img{
	float: left;
}
.navLeft li a{
	text-align: center;
	line-height:27px;
	color:#999;font-size:12px;
	float: right;
}
.navLeft a:hover{
	color:#C40000;
	text-decoration:underline;
}
.navLeft .nav_text{
	width:85px;
	color:#999;font-size:12px;
	margin-left:20px;
}
.navRight li{
	width:55px;height:27px;
	float:left;
	text-align: center;
	line-height:27px;
	color:#999;
	margin-left:12px;
}
.navRight li img{
	float: left;
	margin-right:5px;
	margin-top: -3px;
}
.navRight li a{
	display: block;
	text-align: center;
	line-height:27px;
	color:#999;font-size:12px;
	float:left;
}
.navRight .nav_text1{
	width:90px;
	color:#999;font-size:12px;
}
.navRight .nav_text2{
	width:10px;
}
.navRight .nav_text3{
	width:36px;
}
.navRight .nav_text4{
	width:36px;
}
.navRight a:hover{
	color:#C40000;
	text-decoration:underline;
}
/*搜索栏*/
.search{
	width:100%;height:130px;
	background:white;
}
.searchbox{
	width:1230px;height:102px;
	margin:0 auto;
	padding-bottom:28px;	
}
.searchLeft{
	width:300px;height:130px;
	float:left;
}
.searchMiddle{
	width:625px;height:100px;
