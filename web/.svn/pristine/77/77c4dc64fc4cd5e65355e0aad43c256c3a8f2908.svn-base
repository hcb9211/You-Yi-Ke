body,p,ul,li,dl,dd,dt,h1,h2,h3,h4,h5,h6,input{
	margin:0;
	padding:0;
	list-style:none;
    font-family: Arial,"微软雅黑"
}
a{
	text-decoration : none;
}
a img{
	border:0;
}
input,img{
	vertical-align: middle;
}
.box{
	width:100%;height:auto;
	background:#f2f2f2;
}
.top{
	width:100%;height:100px;
	background:#F6DBE4;
