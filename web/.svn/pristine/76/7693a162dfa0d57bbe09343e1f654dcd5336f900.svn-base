.cent{
    width: 100%;
    height: auto;
    background:#f2ecec;
}
.box{
    width:1200px;
    height:auto;
    margin:0 auto;
    overflow: hidden;
}
.cptx1{
    width: 300px;
    height: 90px;
    text-align: center;
    line-height:90px;
    background: #00a885;
    color: #f7f3f3;
    position: relative;
    top:35px;
    font-size: 20px;
}
.cptx1 img{
    position: absolute;
    top:25px;
    left: 50px;
    width: 40px;
    height: 40px;
}
.box .top{
    width:300px;
    height:420px;
    margin-top: 35px;
}
.top .btn{
    width:300px;
    height:70px;
    background: #fcfeff;
    text-align: center;
    line-height:70px;
}
.btn a{
    width:300px;
    height:70px;
    background: #fcfeff;
    text-align: center;
    line-height:70px;
    display: block;
}
.box .bottom{
    width:880px;
    height:auto;
    float: right;
    margin-top:-510px;
}
.bottom .con{
    width:880px;
    height:auto;
    background: #fff;
    display: none;
}
.top .active{
    border-left:5px solid #00a885 ;
    background:#d0d0d0;
    width: 295px;
}
.cf:after{
    content:"";
    display: block;
    clear:both;
}
